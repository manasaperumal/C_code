#include <stdio.h>
#include <stdlib.h>

int main() {
    int arr[100];  // max 100 elements
    int n = 0, target;

    // Read entire line of numbers (array)
    while (scanf("%d", &arr[n]) == 1) {
        char ch = getchar();
        if (ch == '\n') break; // stop at end of first line
        n++;
    }
    n++; // include last read value

    // Read target value
    scanf("%d", &target);

    int found = 0;

    // Brute-force check
    for (int i = 0; i < n - 1 && !found; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] + arr[j] == target) {
                printf("Index1: %d\n", i); // 1-based
                printf("Index2: %d\n", j );
                found = 1;
                break;
            }
        }
    }

    if (!found) {
        printf("No two sum solution\n");
    }

    return 0;
}
